/*表格修改*/
.el-table { border: none;}
.el-table table { border-collapse: separate;}
.el-table::after, .el-table::before{ background-color: transparent;}
.el-table__fixed-header-wrapper thead div, .el-table__header-wrapper thead div { background-color: #effaff; color: #007dc4;}
.el-table th { background-color: #effaff; color: #007dc4; border:none; border-left: 1px solid #a5defa; border-top: 1px solid #a5defa;}
.el-table th.is-leaf { border-bottom: 1px solid #a5defa;}
.el-table table thead tr th:nth-last-child(1) { border-right: 1px solid #a5defa;}
.el-table--border td { border: none; border-right:1px solid #dfe6ec; border-bottom:1px solid #dfe6ec;}
.el-table .cell,.el-table th>div { padding-left:5px; padding-right:5px;}
.el-table__body { border-left: 1px solid #dfe6ec;}
.el-checkbox__inner { border-radius: 0;}
.el-table.el-table--striped .el-table__body tr:nth-child(2n) td{background: #f9f9f9;}
/*对话框添加border-top*/
/*.el-message-box, .el-dialog { border-top: 5px solid #0C9FEC;}*/

/*el-tooltip样式重置*/
.el-tooltip__popper.is-light,.el-tooltip__popper.is-dark { background: #eff8ff; color: #555555; border: 1px solid #9cbedd;}
.popper__arrow { border-top-color: #9cbedd!important;}
.popper__arrow:after { border-top-color: #eff8ff!important;}

/*新增编辑对话框样式*/
.el-dialog{ width: 338px; overflow: hidden; border-radius: 5px; transform:translate(-50%, -50%); }
.el-dialog .el-dialog__header { padding: 0 20px; height: 49px; line-height: 49px; background: url("../../imgs/dialog-head.png") no-repeat;}
.el-dialog .el-dialog__title {color: #FFF; font-weight: normal;}
.el-dialog .el-dialog__close{ margin-top: 16px; font-size: 16px; color: #FFF;}
.el-dialog .el-dialog__headerbtn:hover .el-dialog__close { color: #FFF;}
.el-dialog .el-dialog__body { padding: 24px 20px 4px 20px;}
.el-dialog__body .el-form-item { margin-bottom: 16px;}
.el-dialog__body .el-form-item.is-required .el-form-item__label:before { margin-right: 10px;}
.el-dialog__body .el-form-item__label { padding: 10px 6px 10px 0; font-size: 14px; color: #666666;}
.el-dialog__body .el-input__inner { padding: 3px 5px; height: 34px; color: #666666;}
.el-dialog__body .el-select { width: 100%;}
.el-dialog__body .el-select .el-input__icon { width: 34px; height: 34px; line-height: 34px;}
.el-dialog__footer { padding: 0 50px 20px; overflow: hidden;}
.el-dialog__footer .el-button { padding: 9px 30px;}
.el-dialog__footer .el-button:nth-of-type(1) { float: left;}
.el-dialog__footer .el-button:nth-of-type(2) { float: right;}

/*confirm确认取消框样式*/
.el-message-box { overflow: inherit; position: relative; width: 298px; border-radius: 5px;}
.el-message-box .el-message-box__header { position: absolute; top: -39px; padding: 0; width: 100%; height: 77px; background-image: url("../../imgs/messageBox-icon.png"); background-repeat: no-repeat; background-size: 117px 77px; background-position: 90px 0;}
.el-message-box .el-message-box__header .el-message-box__title,.el-message-box .el-message-box__header .el-message-box__headerbtn { display: none;}
.el-message-box .el-message-box__content { margin-top: 38px; padding: 20px 30px; height: 46px; line-height: 38px; text-align: center;}
.el-message-box .el-message-box__message { display: inline-block; color: #666;}
.el-message-box .el-message-box__message p { line-height: 20px;}
.el-message-box .el-message-box__btns { padding: 0 40px 20px; overflow: hidden; text-align: center;}
.el-message-box .el-message-box__btns .el-button { padding: 9px 30px;}
.el-message-box .el-message-box__btns .el-button:nth-of-type(1) { margin-right: 28px;}
.el-message-box .el-message-box__btns .el-button:nth-of-type(2) { margin-left: 0;}

  /*按钮重置*/
.el-button { border-radius: 2px; padding: 9px 10px;}
.btns { font-size: 0;}
.btns .el-button { margin: 0; padding: 0; font-size: 14px; width: 90px; height: 34px; line-height: 32px; text-align: center;}
.btns .el-button+.el-button { margin-left: 16px;}
  /*.btns .el-button span { vertical-align: top;}*/
/*.btns .el-button [class*=el-icon-]+span { margin-left: 0;}*/
/*.btns .del-btn [class*=el-icon-]+span { margin-left: 5px;}*/
.el-button--text {  padding: 0;}
.add-btn { background-color: #24c178; border-color: #24c178;}
.add-btn:hover { background-color: #43ca8b; border-color: #43ca8b;}
.add-btn:active { background-color: #1fa667; border-color: #1fa667;}
.del-btn { color: #ff5353; background-color: transparent; border-color: #ff5353;}
.del-btn:hover { color: #fff; background-color: #ff5353; border-color: #ff5353;}
.del-btn:active { color: #fff; background-color: #db4747; border-color: #db4747;}
.btns .el-button.is-disabled { background-color: #f9f9f9; color: #b4b4b4; border-color: #e6e6e6;}
.el-button--success {
  color: #fff;
  background-color: #24c178;
  border-color: #24c178;
}

/*设置状态*/
.el-dropdown-menu { padding: 0; min-width: 75px; font-size: 14px;text-align: center;}
.el-dropdown-menu__item { line-height: 34px; align: center;}
.el-dropdown-menu__item:not(.is-disabled):hover { color: #fff; background-color: #20A0FF;}

/*placeholder字体颜色调整*/
.el-input.is-disabled .el-input__inner::-webkit-input-placeholder{color:#acacac;font-family:"微软雅黑";font-family:"微软雅黑";}
.el-input.is-disabled .el-input__inner::-moz-placeholder{color:#acacac;font-family:"微软雅黑";}
.el-input.is-disabled .el-input__inner:-ms-input-placeholder{color:#acacac;font-family:"微软雅黑";}
.el-input.is-disabled .el-input__inner::placeholder{color:#acacac;font-family:"微软雅黑";}
.el-input__inner::-webkit-input-placeholder{color:#acacac;font-family:"微软雅黑"}.el-input__inner::-moz-placeholder{color:#97a8be;}
.el-input__inner:-ms-input-placeholder{color:#97a8be;}
.el-input__inner::placeholder{color:#acacac;font-family:"微软雅黑";}
/* 输入框 */
.el-textarea__inner , .el-input__inner{border-radius:0;border: 1px solid #e6e6e6;}
.el-select .el-input .el-input__icon{color: #e6e6e6;}

.el-dropdown [class^=el-icon-caret]{color: #d7d7d7;}
/* 下拉 */
.el-dropdown .el-dropdown-link{cursor: pointer;}
